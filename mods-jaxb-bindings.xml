        node="/xsd:schema/xsd:complexType[@name='relatedItemType']/xsd:attribute[@name='type']">
        <property name="riType" />
    </bindings>
    
    <bindings schemaLocation="http://www.loc.gov/standards/mods/v3/mods-3-3.xsd"
        node="/xsd:schema/xsd:complexType[@name='nameType']/xsd:attribute[@name='type']">
        <property name="nType" />
